{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "jsx": "react-jsx",
    "strict": true,
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    // "noEmit": true,
    "baseUrl": ".",
    "paths": { "@/*": ["src/*"] },
    // "types": ["vite/client", "node"]
  },
  "include": ["src", "src/vite-env.d.ts"]
}
